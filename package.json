  "main": "dist/bundle.js",
  "bin": {
    "warsman": "./bin/warsman"
  },
  "scripts": {
    "build": "rollup -c",
    "develop": "rollup -c -w",
    "format": "prettier --write './**/*.{js,ts,json,md}'",
