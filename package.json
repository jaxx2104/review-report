    "@typescript-eslint/typescript-estree": "1.11.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.18.2",
    "rollup": "1.16.4",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-node-resolve": "4.2.4",
