{
  "name": "warsman",
  "version": "0.2.0",
  "description": "Displays the most contributing silent hero in the code review",
  "main": "dist/bundle.js",
  "bin": {
    "warsman": "./bin/warsman"
  },
  "scripts": {
    "build": "rollup -c",
    "develop": "rollup -c -w",
    "format": "prettier --write './**/*.{js,ts,json,md}'",
    "lint": "eslint --fix '**/*.ts'",
    "release": "np",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "license": "MIT",
  "repository": "jaxx2104/warsman",
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "cli",
    "codereview",
    "github",
    "pullrequest"
  ],
  "author": "jaxx2104",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^3.0.0",
    "figlet": "^1.2.1",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.147",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "@typescript-eslint/typescript-estree": "2.7.0",
    "eslint": "5.11.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "np": "5.1.3",
    "prettier": "1.19.1",
    "rollup": "1.27.0",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-typescript": "1.0.0",
    "tslib": "1.9.3",
    "typescript": "3.7.2"
  }
}
